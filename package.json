{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "name": "one-start",
  "repository": "git@10.1.2.7:visual-fe/swap-modules.git",
  "scripts": {
    "merge:changelog": "node ./scripts/cli/merge-changelog.js --path 'packages/*'",
    "remove:empty-pkgs": "node ./scripts/remove-empty-pkgs.js",
    "scfupgrade": "scfupgrade && yarn",
    "check:pkg-circular-depends": "node ./scripts/check-pkg-circular-depends/index.js",
    "check:used-depends-must-in-pkg": "node ./scripts/check-used-depends-must-in-pkg.js",
    "bootstrap": "node ./scripts/bootstrap.js",
    "build:all": "father-build && webpack",
    "build": "father-build",
    "size:build": "cross-env PRO_COMPONENTS_CI='CI' webpack",
    "changelog": "pro-changelog",
    "createRelease": "node ./scripts/createRelease.js",
    "deploy": "deploy-go",
    "format": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "site": "cross-env UMI_ENV=prod dumi build",
    "size": "npm run build && size-limit",
    "start": "npm run dev",
    "start:scripts": "yarn lerna run --scope=@ty-one-start/scripts build:watch",
    "dev": "dumi dev",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "test:update": "umi-test --updateSnapshot",
    "tsc": "tsc --noEmit",
    "tsc:duplicate": "tsc -p ./tests/tsconfig.duplicate.json",
    "update:deps": "yarn upgrade-interactive --latest",
    "lint": "npm run lint:formats && npm run lint:code && npm run lint:style",
    "lint:staged": "lint-staged",
    "lint:formats": "prettier --version && prettier -c \"packages/**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "lint:commit": "commitlint -E HUSKY_GIT_PARAMS",
    "lint:code": "eslint --ext .js,.jsx,.ts,.tsx --format=pretty ./packages",
    "lint:style": "stylelint --syntax less --fix ./packages/**/**.less",
    "clean": "rimraf dist lib",
    "ci": "yarn lint && yarn tsc && yarn check:pkg-circular-depends && yarn check:used-depends-must-in-pkg",
    "cd": "yarn ci && yarn deploy",
    "prepare": "husky install",
    "precommit": "yarn lint-staged",
    "release": "yarn build && yarn release:only",
    "version:only": "lerna version --exact --conventional-commits",
    "release:only": "yarn version:only && yarn publish:only",
    "publish:only": "lerna publish from-git --yes",
    "publish:only:package": "lerna publish from-package --yes",
    "unpublish": "lerna exec -- 'npm unpublish --force --registry=http://npm.tongyu.tech'",
    "gen:api": "cross-env OPEN_API=true umi openapi"
  },
  "peerDependencies": {
    "@ty-fee-tools/scaffolding-upgrade": "1.x"
  },
